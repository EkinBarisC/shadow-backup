<UserControl x:Class="Back_It_Up.Views.UserControls.EncryptUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Back_It_Up.Views.UserControls"
              xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
             mc:Ignorable="d" 
                d:DataContext="{d:DesignInstance local:EncryptUserControl,
                IsDesignTimeCreatable=False}"
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>

        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
        </Grid.RowDefinitions>

        <ui:PasswordBox Grid.Row="0"  Width="300"/>
        <ui:PasswordBox Grid.Row="1" Width="300" />

        <Button Grid.Row="2" Content="Save" HorizontalAlignment="Center" />

    </Grid>
</UserControl>
